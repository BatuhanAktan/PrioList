{
  "manifest_version": 2,
  "name": "PrioList",
  "content_security_policy": "script-src 'self' https://github.com/puppeteer/puppeteer; object-src 'self'",
  "description": "This extension allows you to create a wishlist which is then prioritized by the order of importance",
  "version": "0.0",

  "browser_action": {
   "default_icon": "./visuals/icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "storage",
   "tabs",
   "identity",
   "http://localhost:9222/*"
   ]
}